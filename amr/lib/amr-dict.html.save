    function initialize() {
       document.onkeypress = key_press;
       document.onkeyup = key_up;
       if (document.layers) {
	  document.captureEvents(Event.KEYPRESS);
       }
       update_from_n_days_ago();
       update_to_n_days_ago();
       update_date_add_mod_info();
    }

