    function simple_print_date(date) {
       var monthNames=new Array("Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec");
       var month;
       if (typeof (month = date.getMonth()) !== 'undefined') {
          return monthNames[month] + ' ' + date.getDate() + ', ' + date.getFullYear();
       } else {
	  return "";
       }
    }

    function update_date_add_mod_info() {
       var fromSecondsTime = document.getElementById('ref_time_from').value;
       var toSecondsTime = document.getElementById('ref_time_to').value;

       // var sl = document.getElementById('log');
       // sl.innerHTML = fromSecondsTime + '-' + toSecondsTime;

       var n_topics_added = 0;
       var n_topics_modified = 0;
       var n_changes = 0;

       var n_date_topic_index = 1;
       while ((s = document.getElementById('DATED_TOPIC_' + n_date_topic_index)) != null) {
	  var time_added = s.getAttribute('time_added');
	  var times_modified = s.getAttribute('times_modified');
	  var title = s.getAttribute('core_title');
	  if (time_added && (fromSecondsTime <= time_added) && (time_added <= toSecondsTime)) {
	     s.style.color = 'red';
	     title += '\n(topic added ' + simple_print_date(new Date(time_added * 1000)) + ')';
	     n_topics_added++;
	     // sl.innerHTML += ' T: ' + title;
	  } else if (times_modified) {
	     var time_modified_list = times_modified.split(/\s+/);
	     var matching_time_modified_list = new Array();
	     for (var i=0; i<time_modified_list.length; i++) {
		var time_modified = time_modified_list[i];
		if ((fromSecondsTime <= time_modified) && (time_modified <= toSecondsTime)) {
		   matching_time_modified_list.push(time_modified);
		}
	     }
	     if (matching_time_modified_list.length >= 1) {
		s.style.color = 'green';
	        title += '\n(topic modified ' + simple_print_date(new Date(matching_time_modified_list[0] * 1000));
		for (var i=1; i<matching_time_modified_list.length; i++) {
	           title += '; ' + simple_print_date(new Date(matching_time_modified_list[i] * 1000));
		}
		title += ')';
	        n_topics_modified++;
	     } else {
		s.style.color = '#0000DD';
	     }
	  } else {
	     s.style.color = '#0000DD';
	  }
	  s.setAttribute('title', title);
	  n_date_topic_index++;
       }

       var n_date_body_index = 1;
       while ((s = document.getElementById('DATED_BODY_' + n_date_body_index)) != null) {
	  var time_added = s.getAttribute('time_added');
	  var times_modified = s.getAttribute('times_modified');
	  // sl.innerHTML += ' I: ' + n_date_body_index + ' A: ' + time_added + ' M: ' + times_modified;
	  if (time_added && (fromSecondsTime <= time_added) && (time_added <= toSecondsTime)) {
	     s.style.color = 'red';
	     s.style.display = 'inline';
	     s.innerHTML = ' &nbsp; (added ' + simple_print_date(new Date(time_added * 1000)) + ')';
	     n_changes++;
	     // sl.innerHTML += ' T: ' + time_added;
	  } else if (times_modified) {
	     var time_modified_list = times_modified.split(/\s+/);
	     var matching_time_modified_list = new Array();
	     for (var i=0; i<time_modified_list.length; i++) {
		var time_modified = time_modified_list[i];
		if ((fromSecondsTime <= time_modified) && (time_modified <= toSecondsTime)) {
		   matching_time_modified_list.push(time_modified);
		}
	     }
	     if (matching_time_modified_list.length >= 1) {
		s.style.color = 'green';
	        s.innerHTML = ' &nbsp; (modified ' + simple_print_date(new Date(matching_time_modified_list[0] * 1000));
	        // sl.innerHTML += ' T: ' + matching_time_modified_list[0];
		for (var i=1; i<matching_time_modified_list.length; i++) {
	           s.innerHTML += '; ' + simple_print_date(new Date(matching_time_modified_list[i] * 1000));
	           // sl.innerHTML += ' T: ' + matching_time_modified_list[i];
		}
		s.innerHTML += ')';
	        n_changes += matching_time_modified_list.length;
		s.style.display = 'inline';
	     } else {
		s.style.display = 'none';
		// sl.innerHTML += ' NONE ';
	     }
	  } else {
	     s.style.display = 'none';
	     // sl.innerHTML += ' NONE ';
	  }
	  n_date_body_index++;
       }

       // sl.innerHTML += ' change ' + n_topics_added + ' ' + n_topics_modified + ' ' + n_changes;
       if ((ss = document.getElementById('change_summary')) != null) {
	  var text = 'Additions/modifications in this time period: &nbsp; ';
	  var keywords;
	  if (n_changes) {
	     var s; // plural s
	     var texts = new Array();
	     if (n_topics_added) {
	        if (n_topics_added == 1) {
	           s = '';
	        } else {
	           s = 's';
	        }
	        texts.push(n_topics_added + ' topic' + s + ' added');
	     }
	     if (n_topics_modified) {
	        if (n_topics_modified == 1) {
	           s = '';
	        } else {
	           s = 's';
	        }
	        texts.push(n_topics_modified + ' topic' + s + ' modified');
	     }
	     if (n_changes == 1) {
	        s = '';
	     } else {
	        s = 's';
	     }
	     texts.push('with a total of ' + n_changes + ' change note' + s);
	     text += texts.join(", ");
	  } else {
	     text += 'none';
	  }
	  ss.innerHTML = text;
       }
    }

    function update_from_n_days_ago () {
       var s;
       if ((s = document.getElementById('from_n_days_ago')) != null) {
	  s.value = s.value.replace(/[^0-9]/g, "") || 0;
	  s.value = s.value.replace(/^0+([0-9])/, '$1');
	  var from_n_days_ago = s.value;
          var fromDate = new Date();
	  fromDate.setHours(0);
	  fromDate.setMinutes(0);
	  fromDate.setSeconds(0);
	  fromDate.setDate(fromDate.getDate()-from_n_days_ago);
	  if ((s2 = document.getElementById('from_date')) != null) {
	     s2.innerHTML = simple_print_date(fromDate);
	  }
	  fromSecondsTime = Math.floor(fromDate.getTime() / 1000);
	  if ((s3 = document.getElementById('ref_time_from')) != null) {
	     s3.value = fromSecondsTime;
	  }
       }
    }

    function update_to_n_days_ago () {
       var s;
       if ((s = document.getElementById('to_n_days_ago')) != null) {
	  s.value = s.value.replace(/[^0-9]/g, "") || 0;
	  s.value = s.value.replace(/^0+([0-9])/, '$1');
	  var to_n_days_ago = s.value;
          var toDate = new Date();
	  toDate.setHours(23);
	  toDate.setMinutes(59);
	  toDate.setSeconds(59);
	  toDate.setDate(toDate.getDate()-to_n_days_ago);
	  if ((s2 = document.getElementById('to_date')) != null) {
	     s2.innerHTML = simple_print_date(toDate);
	  }
	  toSecondsTime = Math.floor(toDate.getTime() / 1000);
	  if ((s3 = document.getElementById('ref_time_to')) != null) {
	     s3.value = toSecondsTime;
	  }
       }
    }

    function key_up(e) {

       var keycode = -1;
       if (window.event) {
	  keycode = window.event.keyCode;
       } else if (e) {
	  keycode = e.which;
       }

       var s;
       if ((s = document.getElementById('from_n_days_ago')) == document.activeElement) {
          if (keycode == 13) {
	     s.blur();
	  } else {
	     update_from_n_days_ago();
	     update_date_add_mod_info();
          }
       } else if ((s = document.getElementById('to_n_days_ago')) == document.activeElement) {
	  if (keycode == 13) {
	     s.blur();
	  } else {
	     update_to_n_days_ago();
	     update_date_add_mod_info();
	  }
       }
    }

    function key_press(e) {

       var keycode = -1;
       if (window.event) {
	  keycode = window.event.keyCode;
       } else if (e) {
	  keycode = e.which;
       }

       var s;
       if ((s = document.getElementById('from_n_days_ago')) == document.activeElement) {
	  // special case -> key_up
       } else if ((s = document.getElementById('to_n_days_ago')) == document.activeElement) {
	  // special case -> key_up
       } else {

          // sd = document.getElementById('log'); sd.innerHTML = 'key-press: ' + keycode;
 
          var date = new Date();
          var current_time = date.getTime();
          var time_diff = current_time - last_time;

          // reset jump_string after 1000 milliseconds
          if (time_diff > 1000) {
	     jump_string = '#';
          }
          // sd = document.getElementById('log'); sd.innerHTML = 'key-press: ' + keycode + ' jump_string: ' + jump_string + '  time_diff: ' + time_diff;
          last_time = current_time;

          if (keycode == 13) {
	     // carriage return (enter)
	     jump_string = '#';
	     window.location = jump_string;
          } else if ((keycode == 32) && (jump_string == '#')) {
	     // space bar, no special action, let browser scroll
          } else if ((keycode >= 32) && (keycode <= 126)) {
	     var chr = String.fromCharCode(keycode) + '';
	     jump_string += chr;
             window.location = jump_string;
	     // sd = document.getElementById('log'); sd.innerHTML = 'jump-string: ' + jump_string;
          } else if (keycode == 13) {
	     // enter
	     // sd = document.getElementById('log'); sd.innerHTML = 'key-press: ' + keycode + ' (enter)';
	     jump_string = '#';
          } else {
	     // sd = document.getElementById('log'); sd.innerHTML = 'key-press: ' + keycode + ' (unrecognized shortcut key)';
          }
       }
    }

